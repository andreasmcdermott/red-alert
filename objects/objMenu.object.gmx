    draw_text(cx, 225, "[M] for toggling music");
    draw_set_halign(fa_right);
    draw_text(room_width - 10, 305, "GameMaker: Studio");
    draw_text(room_width - 10, 325, "PixiTracker");
}
else if(page == "highscore")
{
    draw_text(cx, 100, "Highscore");
    draw_text(cx, room_height - 30, "Escape to go back");
    draw_set_font(font0);
    for(var i = 1; i &lt;= 10; i++)
    {
        draw_set_halign(fa_left);
        draw_text(10, 106 + i * 19, string(i) + ".");
        draw_set_halign(fa_center);
        draw_text(room_width / 2, 106 + i * 19, highscore_name(i));
        draw_set_halign(fa_right);
        draw_text(room_width - 10, 106 + i * 19, string(highscore_value(i)));
    }
}
else if(page == "controls")
{
    draw_text(cx, 100, "Controls");
    draw_text(cx, room_height - 30, "Escape to go back");
    draw_set_font(font0);
    draw_text(cx, 125, "Left mouse button: Fire weapon");
    draw_text(cx, 145, "WASD: Move character");
    draw_text(cx, 165, "Arrow keys: Move character");
    draw_text(cx, 185, "Mouse: Look around");
    draw_text(cx, 205, "Escape: Go to menu");
    draw_text(cx, 225, "M: Toggle music");
}
    
if(blink)
{
    draw_set_color(c_red);
    draw_set_alpha(0.075);
    draw_rectangle(0, 0, room_width, room_height, false);
    draw_set_alpha(1);
}

